  "bugs": {
    "url": "https://github.com/perry-mitchell/buttercup-core-web/issues"
  },
  "homepage": "https://github.com/perry-mitchell/buttercup-core-web#readme",
  "dependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "buttercup": "~0.17.0",
    "crypto-browserify": "^3.11.0",
    "fs-symlink": "^1.2.1",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "json-loader": "^0.5.4",
    "load-grunt-tasks": "^3.3.0",
    "uglifyjs": "^2.4.10",
