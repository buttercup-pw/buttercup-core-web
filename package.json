{
  "name": "buttercup-web",
  "version": "0.10.1",
  "description": "Buttercup core library for the web.",
  "main": "build/buttercup.js",
  "scripts": {
    "test": "npm run build && npm run karma",
    "test:ci": "npm test",
    "postinstall": "npm run build",
    "build": "webpack && npm run minify",
    "karma": "karma start",
    "dev": "webpack --progress",
    "minify": "uglifyjs --compress --screw-ie8 --output ./build/buttercup.min.js -- ./build/buttercup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buttercup-pw/buttercup-core-web.git"
  },
  "keywords": [
    "buttercup",
    "password",
    "security",
    "encryption"
  ],
  "author": "Perry Mitchell <perry@perrymitchell.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buttercup-pw/buttercup-core-web/issues"
  },
  "homepage": "https://github.com/buttercup-pw/buttercup-core-web#readme",
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "buttercup": "~0.25.0",
    "crypto-browserify": "^3.11.0",
    "json-loader": "^0.5.4",
    "uglifyjs": "^2.4.10",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "mocha": "^3.1.2"
  }
}
