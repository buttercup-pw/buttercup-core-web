{
  "name": "buttercup-web",
  "version": "0.25.2",
  "description": "Buttercup core library for the web.",
  "main": "build/buttercup.min.js",
  "scripts": {
    "build": "npm run clean && webpack",
    "clean": "rimraf build/*.*",
    "dev": "webpack --progress --watch",
    "generate:docs": "jsdoc2md 'source/**/*.js' > API.md",
    "karma": "karma start",
    "prepublish": "npm run build",
    "test": "npm run test:lint && npm run karma -- --single-run",
    "test:ci": "npm test",
    "test:lint": "eslint 'source/**'",
    "test:watch": "npm run karma"
  },
  "files": [
    "build/buttercup.js",
    "build/buttercup.min.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buttercup-pw/buttercup-core-web.git"
  },
  "keywords": [
    "buttercup",
    "password",
    "security",
    "encryption"
  ],
  "author": "Perry Mitchell <perry@perrymitchell.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buttercup-pw/buttercup-core-web/issues"
  },
  "homepage": "https://github.com/buttercup-pw/buttercup-core-web#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "crypto-browserify": "~3.11.0",
    "dropbox-fs": "~0.0.4",
    "eslint": "^3.13.0",
    "fuse.js": "~2.6.2",
    "jsdoc-to-markdown": "^2.0.1",
    "json-loader": "~0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^1.8.1",
    "mocha": "^3.1.2",
    "node-noop": "^1.0.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "uglify-js": "^2.8.22",
    "uglifyjs": "~2.4.10",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "~2.4.1"
  },
  "dependencies": {
    "buttercup": "~0.38.0"
  }
}
