{
  "name": "buttercup-web",
  "version": "0.6.0",
  "description": "Buttercup core library for the web.",
  "main": "source/index.js",
  "scripts": {
    "test": "npm run nodeunit",
    "postinstall": "npm run build",
    "build": "webpack && npm run minify",
    "minify": "uglifyjs --compress --screw-ie8 --output build/buttercup.min.js -- build/buttercup.js",
    "nodeunit": "node testing/setup/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buttercup-pw/buttercup-core-web.git"
  },
  "keywords": [
    "buttercup",
    "password",
    "security",
    "encryption"
  ],
  "author": "Perry Mitchell <perry@perrymitchell.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buttercup-pw/buttercup-core-web/issues"
  },
  "homepage": "https://github.com/buttercup-pw/buttercup-core-web#readme",
  "dependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "buttercup": "~0.19.0",
    "crypto-browserify": "^3.11.0",
    "json-loader": "^0.5.4",
    "uglifyjs": "^2.4.10",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "chromedriver": "^2.21.2",
    "nodeunit": "^0.9.1",
    "selenium-webdriver": "^2.53.1"
  }
}
